{
  "name": "@relational-fabric/weft",
  "version": "0.0.3",
  "description": "Core types and utilities for Relational Fabric",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./compat": {
      "types": "./dist/compat/index.d.ts",
      "import": "./dist/compat/index.js",
      "default": "./dist/compat/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "echo \"No tests specified\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "deploy": "npm publish"
  },
  "dependencies": {
    "@relational-fabric/filament": "*"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.20",
    "@types/object-hash": "^3.0.6",
    "immutable": "^5.1.3",
    "lodash": "^4.17.21",
    "nanoid": "^5.1.5",
    "object-hash": "^3.0.0",
    "typescript": "^5.8.3"
  }
}