{
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        "dist/**/*.js",
        "dist/**/*.d.ts"
      ]
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ]
    },
    "test": {
      "dependsOn": [
        "build",
        "^test"
      ]
    },
    "test:coverage": {
      "dependsOn": [
        "build",
        "^test:coverage"
      ]
    },
    "deploy": {
      "dependsOn": [
        "build",
        "^deploy"
      ]
    }
  }
}